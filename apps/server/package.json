{
  "name": "@stackone/mcp-server",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "bun run --watch src/index.ts",
    "start": "bun run src/index.ts",
    "spawn": "bun run src/spawn-server.ts"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.18.0",
    "@stackone/mcp-connectors": "workspace:*",
    "express": "^4.21.2",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@types/express": "^5.0.0",
    "@types/node": "^24.10.1"
  },
  "devEngines": {
    "runtime": [
      {
        "name": "node",
        "version": "^24.11.0",
        "onFail": "download"
      },
      {
        "name": "bun",
        "version": "^1.3.2",
        "onFail": "download"
      }
    ]
  }
}
