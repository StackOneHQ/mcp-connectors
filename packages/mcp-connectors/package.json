{
  "name": "@stackone/mcp-connectors",
  "version": "0.0.24",
  "description": "MCP connectors for disco.dev",
  "module": "./dist/index.js",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": "./src/index.ts",
    "./package.json": "./package.json"
  },
  "type": "module",
  "files": [
    "src",
    "dist",
    "LICENSE"
  ],
  "scripts": {
    "build": "bun -b tsdown",
    "prepublishOnly": "bun run build",
    "test": "vitest --run",
    "typecheck": "tsgo --noEmit"
  },
  "dependencies": {
    "@1password/connect": "catalog:prod",
    "@linear/sdk": "catalog:prod",
    "@modelcontextprotocol/sdk": "catalog:prod",
    "@orama/orama": "catalog:prod",
    "modal": "catalog:prod",
    "node-html-parser": "catalog:prod",
    "openai": "catalog:prod",
    "zod": "catalog:prod"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/StackOneHQ/mcp-connectors.git"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "connectors",
    "stackone",
    "saas",
    "integrations",
    "disco"
  ],
  "author": "StackOne",
  "license": "Apache-2.0",
  "bugs": "https://github.com/StackOneHQ/mcp-connectors/issues",
  "homepage": "https://github.com/StackOneHQ/mcp-connectors#readme",
  "publishConfig": {
    "exports": {
      ".": "./dist/index.js",
      "./package.json": "./package.json"
    }
  },
  "devEngines": {
    "runtime": [
      {
        "name": "node",
        "version": "^24.11.0",
        "onFail": "download"
      },
      {
        "name": "bun",
        "version": "^1.3.2",
        "onFail": "download"
      }
    ]
  },
  "devDependencies": {}
}
